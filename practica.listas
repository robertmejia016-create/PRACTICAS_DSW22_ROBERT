fun main(){
    val numerosPares =mutableListOf<Int>()
    
    for(num in 1..100) {
        if(num % 2== 0) {
            numerosPares.add(num)
        }

    }
    println("cantidad de numeros pares entre 1 y 100: ${numerosPares.size}")
    println(numerosPares)
    println("tama√±o de la lista: ${numerosPares.size}")
}
